{
  "name": "web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/web",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm vite build",
        "cwd": "apps/web"
      },
      "outputs": ["{projectRoot}/.output"]
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm vite dev --port 3000",
        "cwd": "apps/web"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm vite dev --port 3000",
        "cwd": "apps/web"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm vite preview --port 3000",
        "cwd": "apps/web"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["apps/web/app/**/*.ts", "apps/web/app/**/*.tsx"]
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm vitest run",
        "cwd": "apps/web"
      }
    },
    "test:watch": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm vitest",
        "cwd": "apps/web"
      }
    },
    "test:coverage": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm vitest run --coverage",
        "cwd": "apps/web"
      },
      "outputs": ["{projectRoot}/coverage"]
    }
  }
}
