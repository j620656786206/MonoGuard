{
  "framework": "nextjs",
  "buildCommand": "cd ../.. && pnpm install && pnpm nx build frontend",
  "startCommand": "cd ../.. && pnpm nx start frontend",
  "installCommand": "cd ../.. && pnpm install",
  "outputDirectory": ".next",
  "nodeVersion": "18",
  "cache_dependencies": true,
  "disable_dockerfile": true,
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}
